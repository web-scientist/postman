{
    "name": "webscientist/postman",
    "type": "library",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ramsey/uuid": "^4.2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WebScientist\\Postman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Chinmay Purav",
            "email": "experiment@webscientist.xyz"
        }
    ],
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
